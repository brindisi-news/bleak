	<link rel="stylesheet" href="{{asset "css/style.css"}}" media="none" onload="if(media!='all')media='all'"><noscript><link rel="stylesheet" href="{{asset "css/style.css"}}"></noscript>

	{{ghost_head}}
</head>

<body class="{{body_class}}">
	<div id="wrapper">
		<div id="ajax-container">
			<nav id="nav" class="nav">
				<div class="nav-logo">
					<a href="{{@site.url}}">
						{{#if @site.logo}}
						<img src="{{@site.logo}}" alt="{{t "Logo" }}" />
						{{/if}}
						<span class="name">{{@site.title}}</span>
						{{#if @site.description}}
							<span class="description">{{@site.description}}</span>
						{{/if}}
					</a>
				</div>
				<div class="nav-menu">
					{{#if @site.navigation}}
					<a class="menu" data-action="menu" data-target="menu"><i class="icon-menu"></i></a>
					{{/if}}
				</div>
			</nav>
			{{{body}}}
			<div id="body-class" style="display: none;" class="{{body_class}}"></div>
			<footer id="footer">
				<section class="credits">
					<span class="credits-software"><a href="https://brindisi.tk/privacy" target="_blank" rel="nofollow">Privacy Policy Cookies e GDPR</a> {{t " - " }}<a href="https://brindisi.tk" target="_blank" rel="nofollow">Brindisi.tk</a> {{t "Â© 2019" }}</span>
				</section>
			</footer>
			{{#if @site.navigation}}
			{{navigation}}
			{{/if}}
		</div>
	</div>
	{{ghost_foot}}
	<script async="true" type="text/javascript" src="{{asset "js/dependencies.js"}}"></script>
	<script async="true" type="text/javascript" src="{{asset "js/script.js"}}"></script>
</body>
</html>
